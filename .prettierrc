{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "printWidth": 80,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "overrides": [{
    "files": [
      "**/*.js"
    ],
    "options": {
      "trailingComma": "es5"
    }
  }]
}